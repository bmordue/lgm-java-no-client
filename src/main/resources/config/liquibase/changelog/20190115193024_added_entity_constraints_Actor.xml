<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Actor.
    -->
    <changeSet id="20190115193024-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="turn_id"
                                 baseTableName="actor"
                                 constraintName="fk_actor_turn_id"
                                 referencedColumnNames="id"
                                 referencedTableName="game_turn"/>

        <addForeignKeyConstraint baseColumnNames="player_id"
                                 baseTableName="actor"
                                 constraintName="fk_actor_player_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>

    </changeSet>
</databaseChangeLog>
